{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "src/index.ts",
  "scripts": {
    "advzip": "node ./scripts/advzip.js",
    "build:dev": "webpack --mode=development",
    "build:prod": "export OPTIMIZE='true' && webpack --mode=production --node-env=production && rm build/bundle.js && yarn zip && yarn advzip",
    "build:prod:no-optimize": "export OPTIMIZE='false' && webpack --mode=production --node-env=production && yarn eslint ./build/bundle.js --fix --quiet --no-ignore",
    "commitlog": "node ./scripts/commitLog.js",
    "deploy": "git push origin `git subtree split --prefix build master`:gh-pages --force",
    "postinstall": "husky install",
    "start": "webpack serve",
    "start:visuals": "export DESIGN_MODE='visuals' && webpack serve",
    "start:audio": "export DESIGN_MODE='audio' && webpack serve",
    "zip": "node ./scripts/zip.js",
    "lint": "eslint ./src --fix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@mracette/eslint-config": "^0.0.14",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "@webpack-cli/generators": "^2.2.0",
    "advzip-bin": "^2.0.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-tsdoc": "^0.2.16",
    "html-webpack-plugin": "^5.3.2",
    "htmlmin": "0.0.7",
    "husky": "^7.0.2",
    "mini-css-extract-plugin": "^2.2.2",
    "prettier": "^2.0.5",
    "react-dev-utils": "^11.0.4",
    "style-loader": "^3.2.1",
    "terser-webpack-plugin": "^5.1.4",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "crco-utils": "^2.0.7",
    "stats.js": "^0.17.0"
  }
}
