{
  "name": "bardo",
  "version": "0.0.1",
  "description": "Bardo",
  "main": "src/index.ts",
  "scripts": {
    "advzip": "node ./scripts/advzip.js",
    "build:dev": "webpack --mode=development",
    "build:prod": "export OPTIMIZE='true' && webpack --mode=production --node-env=production && rm build/bundle.js && yarn zip && yarn advzip",
    "build:prod:no-optimize": "export OPTIMIZE='false' && webpack --mode=production --node-env=production && yarn eslint ./build/bundle.js --fix --quiet --no-ignore",
    "compress-svg": "node ./scripts/setTypeToModule.js && node ./scripts/compressAllSvg.js && yarn lint",
    "commit-log": "node ./scripts/commitLog.js",
    "commit-log:append": "node ./scripts/commitLogAppend.js",
    "deploy": "git push origin `git subtree split --prefix build master`:gh-pages --force",
    "lint": "eslint {src,svg,studio}/**/*.ts scripts/**/*.js --fix",
    "start": "webpack serve",
    "start:studio": "export DESIGN_MODE=studio && webpack serve",
    "zip": "node ./scripts/zip.js"
  },
  "author": "Mark Racette",
  "type": "module",
  "license": "ISC",
  "devDependencies": {
    "@mracette/eslint-config": "^0.0.18",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@webpack-cli/generators": "^2.5.0",
    "advzip-bin": "^2.0.0",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.16",
    "html-webpack-plugin": "^5.5.0",
    "htmlmin": "0.0.7",
    "husky": "^8.0.1",
    "mini-css-extract-plugin": "^2.6.1",
    "points-on-path": "^0.2.1",
    "prettier": "^2.7.1",
    "react-dev-utils": "^12.0.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.4",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0"
  },
  "dependencies": {
    "bounded-sine": "^0.0.2",
    "crco-utils": "^2.0.27",
    "stats.js": "^0.17.0"
  }
}
